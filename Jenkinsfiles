node ('workstation') {
    env.sample_url = "ss.scripted.com"
    try {
        withCredentials([usernameColonPassword(credentialsId: 'SSH', variable: 'USERPASS')]) {
            stage ("one") {
                echo "stage one"
                sh "env"
                sh 'env > /tmp/env'
            }
        }
        stage ("two") {
            echo "stage two"
        }
        catch (e) {
            stage ("three") {
                echo "i wil run on failures"
            }
        }
    }
}
